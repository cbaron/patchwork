<li data-js="container" class="delivery-options col-xs-12">
    <div class="error">Please select a valid option.</div>
    <div class="row">
        <div class="share-label col-sm-3 vcenter">
            <div>{{label}}</div>
            <div>
                <span>{{humanStartdate}}</span>
                <span>-</span>
                <span>{{humanEnddate}}</span>
            </div>
            <div>{{duration}} weeks</div>
            <div class="option-total" data-js="optionTotal"></div>
        </div><!--
        --><div data-js="options" class="col-sm-9 vcenter"></div>
    </div>
    <div data-js="feedback" class="feedback-message col-sm-9 col-sm-offset-3"></div>
</li>
