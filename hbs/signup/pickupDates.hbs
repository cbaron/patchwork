<li data-js="container" class="share-dates col-xs-12">
    <div class="row">
        <div class="share-label vcenter col-sm-2">
            <div>{{label}}</div>
            <div>
                <span>{{humanStartdate}}</span>
                <span>-</span>
                <span>{{humanEnddate}}</span>
            </div>
            <div>{{duration}} weeks</div>
        </div>
        <div class="vcenter col-sm-7 col-sm-offset-1 col-xs-10 col-xs-offset-1">
            <div class="row" data-js="dates"></div>
        </div>
    </div>
    <div class="error">Please select at least one date to receive a share.</div>
</li>
